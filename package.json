{
  "name": "tarojs-router-next",
  "version": "1.3.1",
  "description": "Taro 路由增强，任意传参、跳转页面取值、路由中间件",
  "author": "lblblong",
  "license": "MIT",
  "repository": "lblblong/tarojs-router-next",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "keywords": [
    "taro",
    "路由"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "serve": "microbundle-crl watch --no-compress --format modern,cjs",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "doc": "typedoc --exclude **/node_modules/** --excludeNotExported --stripInternal --out docs src/index.ts"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/koa-compose": "^3.2.5",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3",
    "typedoc": "^0.19.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@tarojs/react": "^3.1.1",
    "@tarojs/taro": "^3.1.1",
    "react": "^17.0.1"
  },
  "dependencies": {
    "@tarojs/react": "^3.1.1",
    "@tarojs/taro": "^3.1.1",
    "koa-compose": "^4.1.0",
    "react": "^17.0.1"
  },
  "files": [
    "dist"
  ]
}
